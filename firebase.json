{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ]
    }
  ],
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/oauth/success",
        "destination": "/success.html"
      },
      {
        "source": "/squareProxy/**",
        "function": "squareProxy"
      },
      {
        "source": "**",
        "destination": "/success.html"
      }
    ]
  }
}